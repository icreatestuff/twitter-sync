{
    "name": "boxhead/twitter-sync",
    "description": "Communicate and process data from the Twitter API",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "twitter"
    ],
    "support": {
        "docs": "https://github.com/weareboxhead/twitter-sync/blob/master/README.md",
        "issues": "https://github.com/weareboxhead/twitter-sync/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Boxhead",
            "homepage": "https://boxhead.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "abraham/twitteroauth": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "boxhead\\twittersync\\": "src/"
        }
    },
    "extra": {
        "name": "Twitter Sync",
        "handle": "twitter-sync",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/weareboxhead/twitter-sync/master/CHANGELOG.md",
        "components": {
            "twitterSyncService": "boxhead\\twittersync\\services\\TwitterSyncService"
        },
        "class": "boxhead\\twittersync\\TwitterSync"
    }
}